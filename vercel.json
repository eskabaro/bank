{
   "version": 2,
   "builds": [
      {
         "src": "server.js",
         "use": "@vercel/node",
         "config": {
            "includeFiles": [
               "server/db.json"
            ]
         }
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "server.js"
      }
   ]
}